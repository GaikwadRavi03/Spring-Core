<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="employee" class="com.bridgelabz.require_anno.Employee">
		<property name="id" value="03" />
		<property name="name" value="Ravi" />
	</bean>

	<bean id="address" class="com.bridgelabz.autowire_anno.Address">
		<property name="city" value="pune" />
		<property name="state" value="MH" />
		<property name="zip" value="411041" />
	</bean>

</beans>