package com.bridgelabz.diconstructor;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class Exam {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ApplicationContext context = new ClassPathXmlApplicationContext("beanssetter.xml");
		Student1 s1 = context.getBean("Student1", Student1.class);
		s1.displayStudentInfo();

		Student1 s2 = context.getBean("Student2", Student1.class);
		s2.displayStudentInfo();
	}

}